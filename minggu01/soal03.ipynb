   "source": [
    "import math\n",
    "\n",
    "modalAwal = 200000000\n",
    "targetCuan = 400000000\n",
    "bunga = 0.10\n",
    "\n",
    "waktu = math.log(targetCuan/ modalAwal) / math.log(1 + bunga)\n",
    "\n",
    "print(\"\\n==JAWABAN==\")\n",
    "print(\"Dibutuhkan waktu:\",f\"{waktu:.2f} tahun\")"
   ]
